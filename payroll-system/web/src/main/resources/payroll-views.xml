<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="WelCome" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="welcomePage" />
	</bean>

	<bean id="companyInfo" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="companyInfoPage" />
	</bean>

	<bean id="companyEarningAndDeduction" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="companyEarningAndDeductionPage" />
	</bean>
	<bean id="companyDepartment" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="companyDepartmentPage" />
	</bean>

	<bean id="employeeSearch" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="employeeSearchPage" />
	</bean>
	<bean id="companyPayrollSchedule" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="companyPayrollSchedulePage" />
	</bean>

	<bean id="companyPayrollScheduleNormal" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="companyPayrollScheduleNormalPage" />
	</bean>

	<bean id="companyWorkersCompensation" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="companyWorkersCompensationPage" />
	</bean>

	<bean id="hello" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="helloPage" />
	</bean>

	<bean id="state" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="statePage" />
	</bean>
	<bean id="stateReditect" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/PayrollSystem/state/getStates" />
	</bean>


	<bean id="login" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="loginPage" />
	</bean>

	<bean id="pinVerification" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="pinPage" />
	</bean>

	<bean id="forgotPasswordStep1" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotPasswordStep1Page" />
	</bean>
	<bean id="forgotPasswordStep2" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotPasswordStep2Page" />
	</bean>
	<bean id="forgotPasswordStep3" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotPasswordStep3Page" />
	</bean>
	<bean id="resetPassword" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="resetPasswordPage" />
	</bean>

	<bean id="forgotUserNameStep1" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotUserNameStep1Page" />
	</bean>
	<bean id="forgotUserNameStep2" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotUserNameStep2Page" />
	</bean>
	<bean id="forgotUserNameStep3" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotUserNameStep3Page" />
	</bean>

	<bean id="forgotPinStep1" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotPinStep1Page" />
	</bean>
	<bean id="forgotPinStep2" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotPinStep2Page" />
	</bean>
	<bean id="forgotPinStep3" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="forgotPinStep3Page" />
	</bean>
	<bean id="resetPin" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="resetPinPage" />
	</bean>

	<bean id="logout" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="logoutPage" />
	</bean>

	<!-- Redirect view -->
	<bean id="logoutReditect" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="logoutReditectPage" />
	</bean>

	<bean id="accessDeny" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="accessDeniedPage" />
	</bean>

	<bean id="multiReport"
		class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
		<property name="url" value="classpath:jasper/city-details.jrxml" />
		<property name="reportDataKey" value="datasource" />
	</bean>

	<bean id="cityReport" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="cityReportPage" />
	</bean>

	<!-- Error Views -->
	<bean id="error500" class="org.springframework.web.servlet.view.tiles2.TilesView">
		<property name="url" value="error500Page" />
	</bean>

</beans>